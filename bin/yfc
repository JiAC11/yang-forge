#!/usr/bin/env node

require('..')
  .define('origin', __dirname)
  .load('!core ../core.yaml')
  .then( function (engine) {
    var core = engine.create('forge');
	core.enable('cli', null, core.access('yangforge'), process.argv);
	engine.run(core);
  })
  .catch( function (err) { console.error(err); } );
